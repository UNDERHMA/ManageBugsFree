select max(bug_report_id) from bugreports;
select max(lbt_report_id) from lbtreports;
select max(br_note_id) from bugreportadditionalnotes;
select max(lbt_note_id) from lbtreportadditionalnotes;
select max(br_similar_bug_reports_id) from brsimilarbugreports;
select max(lbt_similar_bug_reports_id) from lbtsimilarbugreports;
select max(brid_lbt_link_id) from bridlbtlink;
select max(user_id) from users;

SELECT setval('bug_reports_sequence', 34, true);
SELECT setval('lbt_reports_sequence', 37, true);
SELECT setval('bug_reports_additional_notes_sequence', 98, true);
SELECT setval('lbt_reports_additional_notes_sequence', 1, true);
SELECT setval('br_similar_bug_reports_sequence', 3, true);
SELECT setval('lbt_similar_bug_reports_sequence', 1, true);
SELECT setval('bridlbtlink_sequence', 36, true);
SELECT setval('users_sequence', 7, true);




